class Solution:
    def maximumTripletValue(self, nums: List[int]) -> int:
        n = len(nums)
        res = 0
        first = nums[0]
        for j in range(1, n):
            if first < nums[j]:
                first = nums[j]
            for k in range(j+1, n):
                res = max(res, (first - nums[j]) * nums[k])
        return res
        
