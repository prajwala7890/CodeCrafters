class Solution:
    def longestConsecutive(self, nums: List[int]) -> int:
        seq_cnt = 0
        arr = set(nums)
        for i in arr:
            if i-1 not in arr:
                cnt = 0
                while i+ cnt in arr:
                    cnt += 1
                seq_cnt = max(seq_cnt,cnt)
        return seq_cnt
